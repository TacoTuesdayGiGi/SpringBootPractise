<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <!--    连接数据库jar包的路径-->
    <classPathEntry location="D:/install/maven/repository/mysql/mysql-connector-java/8.0.29/mysql-connector-java-8.0.29.jar"/>
    <context id="DB2Tables"  targetRuntime="MyBatis3">
        <commentGenerator>
            <property name="suppressDate" value="false"/>
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>

        <!--数据库连接参数 -->
        <jdbcConnection
                driverClass="com.mysql.jdbc.Driver"
                connectionURL="jdbc:mysql://localhost:3306/reggie?serverTimezone=UTC&amp;useSSL=true"
                userId="root"
                password="root">
        </jdbcConnection>

        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <!-- 实体类的包名和存放路径 -->
        <javaModelGenerator targetPackage="com.co.cn.reggie.entity" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <!-- 生成映射文件*.xml的位置-->
        <sqlMapGenerator targetPackage="mapper" targetProject="src/main/resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <!-- 生成DAO的包名和位置 -->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.co.cn.reggie.mapper" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <!-- tableName：数据库中的表名或视图名；domainObjectName：生成的实体类的类名-->
        <!-- User-->
        <table tableName="user" domainObjectName="User"
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false"
        ><property name="useActualColumnNames" value="false"/>
        </table>
        <!-- ShoppingCart-->
        <table tableName="shopping_cart" domainObjectName="ShoppingCart"
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false"
        ><property name="useActualColumnNames" value="false"/>
        </table>
        <!-- SetmealDish-->
        <table tableName="setmeal_dish" domainObjectName="SetmealDish"
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false"
        ><property name="useActualColumnNames" value="false"/>
        </table>
        <!-- Setmeal-->
        <table tableName="setmeal" domainObjectName="Setmeal"
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false"
        ><property name="useActualColumnNames" value="false"/>
        </table>
        <!-- Orders-->
        <table tableName="orders" domainObjectName="Orders"
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false"
        ><property name="useActualColumnNames" value="false"/>
        </table>
        <!-- OrderDetail-->
        <table tableName="order_detail" domainObjectName="OrderDetail"
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false"
        ><property name="useActualColumnNames" value="false"/>
        </table>
        <!-- Employee-->
        <table tableName="employee" domainObjectName="Employee"
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false"
        ><property name="useActualColumnNames" value="false"/>
        </table>
        <!-- DishFlavor-->
        <table tableName="dish_flavor" domainObjectName="DishFlavor"
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false"
        ><property name="useActualColumnNames" value="false"/>
        </table>
        <!-- Dish-->
        <table tableName="dish" domainObjectName="Dish"
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false"
        ><property name="useActualColumnNames" value="false"/>
        </table>
        <!-- Category-->
        <table tableName="category" domainObjectName="Category"
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false"
        ><property name="useActualColumnNames" value="false"/>
        </table>
        <!-- AddressBook-->
        <table tableName="address_book" domainObjectName="AddressBook"
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false"
        ><property name="useActualColumnNames" value="false"/>
        </table>
    </context>
</generatorConfiguration>